{
    "name": "webapp",
    "version": "0.0.0",
    "scripts": {
        "dev": "SET EMULATED=1* && firebase emulators:start",
        "debug": "SET EMULATED=1* && firebase emulators:start --inspect-functions",
        "dev2": "ng build --watch",
        "dev3": "npm run --prefix functions dev",
        "devall": "wt -d . cmd /k \"npm run dev\" `; split-pane -d . -V -s .4 cmd /k \"npm run dev2\" `; split-pane -d . -H cmd /k \"npm run dev3\"",
        "translate": "attranslate --srcFile=src/assets/i18n/en.json --srcLng=en --srcFormat=nested-json --targetFile=src/assets/i18n/hu.json --targetLng=hu --targetFormat=nested-json --service=manual",
        "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/webapp/stats.json",
        "eslint": "eslint --ext .js,.ts",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build-prod": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.2.9",
        "@angular/common": "~11.2.9",
        "@angular/compiler": "~11.2.9",
        "@angular/core": "~11.2.9",
        "@angular/fire": "^6.1.4",
        "@angular/forms": "~11.2.9",
        "@angular/platform-browser": "~11.2.9",
        "@angular/platform-browser-dynamic": "~11.2.9",
        "@angular/router": "~11.2.9",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "firebase": "^7.0 || ^8.0",
        "ngx-logger": "^4.2.1",
        "ngx-webstorage-service": "^4.1.0",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/architect": ">= 0.900 < 0.1200",
        "@angular-devkit/build-angular": "~0.1102.8",
        "@angular-eslint/builder": "2.0.2",
        "@angular-eslint/eslint-plugin": "2.0.2",
        "@angular-eslint/eslint-plugin-template": "2.0.2",
        "@angular-eslint/schematics": "2.0.2",
        "@angular-eslint/template-parser": "2.0.2",
        "@angular/cli": "~11.2.8",
        "@angular/compiler-cli": "~11.2.9",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "4.16.1",
        "@typescript-eslint/parser": "4.16.1",
        "attranslate": "^1.5.1",
        "codelyzer": "^6.0.0",
        "eslint": "^7.6.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "firebase-tools": "^8.0.0",
        "fuzzy": "^0.1.3",
        "inquirer": "^6.2.2",
        "inquirer-autocomplete-prompt": "^1.0.1",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "open": "^7.0.3",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.1.5",
        "webpack-bundle-analyzer": "^4.4.0"
    }
}
